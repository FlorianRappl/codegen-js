{
  "name": "codegen-lib",
  "version": "2.0.0",
  "description": "A core utility library for generating useful modules at bundle-time.",
  "keywords": [
    "codegen",
    "lib"
  ],
  "author": "Florian Rappl",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "lib/cjs/index.cjs",
  "module": "lib/esm/index.mjs",
  "typings": "lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.mjs",
      "require": "./lib/cjs/index.cjs",
      "types": "./lib/js/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --outDir lib/cjs --module commonjs",
    "build:esm": "tsc --outDir lib/esm --module esnext"
  },
  "engines": {
    "node": ">=16.0"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/FlorianRappl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlorianRappl/codegen-js.git"
  },
  "bugs": {
    "url": "https://github.com/FlorianRappl/codegen-js/issues"
  },
  "files": [
    "lib",
    "src"
  ],
  "devDependencies": {}
}
